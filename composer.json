{
    "name": "jrg-matt/google-bundle",
    "type": "symfony-bundle",
    "description": "Integrate Google Services into Symfony 2 app.",
    "keywords": ["google", "user", "contacts", "url shorter"],
    "homepage": "http://jrg-matt.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Orin Mitchum",
            "email": "mmitchum@jrg-studio.com"
                }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jrg-matt/GoogleApi"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "symfony/security-bundle": ">=2.1",
        "symfony/twig-bundle": ">=2.1",
        "google/apiclient": ">=1.0.4-beta"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "BIT\\GoogleBundle": ""
        }
    },
    "target-dir": "BIT/GoogleBundle"
}
